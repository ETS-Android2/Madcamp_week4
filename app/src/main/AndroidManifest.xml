        android:icon="@mipmap/ic_launcher_whale"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_whale_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Travel"
        android:usesCleartextTraffic="true">

        <activity android:name=".MyPageActivity"></activity>
        <activity android:name=".CheckDayActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".PathMapActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OtherPathActivity" />
        <activity android:name=".MapActivity" />
        <activity android:name=".UserPathActivity" />
        <activity android:name=".UserPlaceActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".PlaceDetailActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ImageFullActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyDCsK1Y92o6guzI4h0jmFHPc6Yz43EUENE" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>